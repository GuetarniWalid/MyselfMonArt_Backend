{
  "name": "dashboard-myselfmonart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cron": "node ace scheduler:run",
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js && node ace scheduler:run",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "copy-env": "dotenv node copy-env.js",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "^4.1.1",
    "@types/cheerio": "^0.22.33",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "node-ssh": "^13.2.0",
    "pino-pretty": "^10.2.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "ssh2": "^1.16.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.4.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "youch": "^3.3.1",
    "youch-terminal": "^2.2.2"
  },
  "dependencies": {
    "@adonisjs/ally": "^4.1.5",
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.2",
    "@adonisjs/mail": "^8.2.1",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.1",
    "@adonisjs/view": "^6.2.0",
    "adonis5-scheduler": "^2.1.1",
    "axios": "^1.6.2",
    "crawlee": "^3.5.8",
    "dashboard-myselfmonart": "file:",
    "google-auth-library": "^9.14.1",
    "googleapis": "^144.0.0",
    "gridjs": "^6.0.6",
    "json-formatter-js": "^2.3.4",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "mysql2": "^3.6.1",
    "openai": "^4.78.1",
    "playwright": "^1.39.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "zod": "^3.24.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "node ace type-check",
      "prettier --write"
    ]
  }
}
